{
    "rewrites": [
      {
        "source": "/",
        "destination": "/public/html/index.html"
      }
    ],
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "app.js"
      }
    ]
  }
  